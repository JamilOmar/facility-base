{
	"tags":
	{
		"allowUnknownTags" : true
	},
	"plugins":[
		 "plugins/markdown"
		,"/opt/homebrew/lib/node_modules/tsdoc/template/plugins/TSDoc.js"
	],
	"opts":
	{
		"template"	:"/opt/homebrew/lib/node_modules/tsdoc/template",
		"recurse"	:"true"
	},
	"templates" : {
		"cleverLinks"     : false,
		"monospaceLinks"  : false
	},
	"source":
	{
		"includePattern": "(\\.d)?\\.ts$"
	},
	"markdown"  : {
		"parser"   : "gfm",
		"hardwrap" : true
	},
	"tsdoc":{
		"source"		:"/Users/falconijo/Documents/Labs/facility-base/src/",
		"destination"	:"/Users/falconijo/Documents/Labs/facility-base/docs",
		"tutorials"		:"",
		"systemName"	: "Facility-Base",
		"footer"		: "",
		"copyright"		: "Copyright Â© 2017 LabShare.",
		"outputSourceFiles" : true,
		"commentsOnly": true
	}
}
